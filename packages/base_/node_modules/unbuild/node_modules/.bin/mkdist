#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/mkdist@2.2.0_typescript@5.6.3_vue@3.5.13_typescript@5.6.3_/node_modules/mkdist/dist/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/mkdist@2.2.0_typescript@5.6.3_vue@3.5.13_typescript@5.6.3_/node_modules/mkdist/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/mkdist@2.2.0_typescript@5.6.3_vue@3.5.13_typescript@5.6.3_/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/mkdist@2.2.0_typescript@5.6.3_vue@3.5.13_typescript@5.6.3_/node_modules/mkdist/dist/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/mkdist@2.2.0_typescript@5.6.3_vue@3.5.13_typescript@5.6.3_/node_modules/mkdist/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/mkdist@2.2.0_typescript@5.6.3_vue@3.5.13_typescript@5.6.3_/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../mkdist/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../../mkdist/dist/cli.cjs" "$@"
fi
