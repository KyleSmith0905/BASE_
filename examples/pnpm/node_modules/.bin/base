#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/packages/base_/bin/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/packages/base_/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/packages/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/node_modules:/mnt/c/Users/yskkyle/OneDrive/node_modules:/mnt/c/Users/yskkyle/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/packages/base_/bin/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/packages/base_/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/packages/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/node_modules:/mnt/c/Users/yskkyle/OneDrive/node_modules:/mnt/c/Users/yskkyle/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/yskkyle/OneDrive/Documents/projects/base-score/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../packages/base_/bin/base_.mjs" "$@"
else
  exec node  "$basedir/../../../../packages/base_/bin/base_.mjs" "$@"
fi
